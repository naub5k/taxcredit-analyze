{
  "routes": [
    {
      "route": "/static/*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "*.{js,css,png,jpg,jpeg,gif,ico,svg,woff,woff2,ttf,eot}",
      "headers": {
        "cache-control": "public, max-age=31536000"
      }
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/static/**", 
      "/images/**", 
      "/api/**",
      "*.{js,css,png,jpg,jpeg,gif,ico,svg,woff,woff2,ttf,eot,map,json,xml,txt}"
    ]
  },
  "mimeTypes": {
    ".json": "application/json",
    ".js": "application/javascript",
    ".css": "text/css"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  }
} 